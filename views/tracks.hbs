<h2>Tracks for Album</h2>

<ul>
    {{#each trackList}}
    <li>
        <h3>{{this.name}}</h3>
        {{#if this.preview_url}}
         <audio controls>
             <source src="{{this.preview_url}}" type="audio/mpeg">
         </audio>
         {{else}}
         <p>NO PREVIEW AVAILABLE!</p>
         {{/if}}
    </li>
    {{/each}}
</ul>